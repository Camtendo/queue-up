{
  "compilerOptions": {
    "target": "ES2023",
    "module": "esnext",
    "moduleResolution": "bundler",
    "outDir": "./server-dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "moduleDetection": "force"
  },
  "include": ["src/backend"],
  "exclude": ["node_modules"]
}
